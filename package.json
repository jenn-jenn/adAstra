    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "express": "^4.17.1",
    "googleapis": "^45.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
